import numpy as np
from sklearn.metrics.pairwise import cosine_similarity
vec1 = np.array([1, 2, 3])
vec2 = np.array([4, 5, 6])
cosine_sim = cosine_similarity([vec1], [vec2])
print(f"Cosine Similarity: {cosine_sim[0][0]}")
correlative_sim = np.dot(vec1, vec2)
print(f"Correlative Similarity (Dot Product): {correlative_sim}")
